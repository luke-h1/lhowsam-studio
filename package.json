{
  "name": "studio",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "sanity start",
    "build": "sanity build public -y",
    "deploy": "sanity deploy",
    "tsc": "tsc --noEmit",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"./**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "lint": "eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "check:node": "check-node-version --node 16 --npm 8",
    "preinstall": "npm run check:node"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/base": "2.33.2",
    "@sanity/client": "3.4.1",
    "@sanity/components": "2.14.0",
    "@sanity/core": "2.33.3",
    "@sanity/default-layout": "2.33.2",
    "@sanity/default-login": "2.33.2",
    "@sanity/desk-tool": "2.33.2",
    "@sanity/eslint-config-studio": "2.0.0",
    "@sanity/icons": "^1.3.6",
    "@sanity/image-url": "1.0.1",
    "@sanity/structure": "2.33.2",
    "@sanity/ui": "0.38.2",
    "@sanity/vision": "2.33.2",
    "check-node-version": "^4.2.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "sanity-plugin-markdown": "^2.1.1",
    "sanity-plugin-media": "1.4.11",
    "sanity-plugin-order-documents": "0.0.23",
    "styled-components": "^5.3.3",
    "typescript": "4.8.3",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@sanity/cli": "^2.33.2",
    "@types/validator": "^13.7.6",
    "prettier": "^2.7.1"
  }
}
