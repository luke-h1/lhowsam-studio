    "@sanity/vision": "2.30.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "sanity-plugin-markdown": "^2.1.1",
    "sanity-plugin-media": "1.4.9",
    "sanity-plugin-order-documents": "0.0.22",
    "styled-components": "^5.3.3",
    "typescript": "4.7.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "@sanity/cli": "^2.30.0",
    "@types/validator": "^13.7.1"
