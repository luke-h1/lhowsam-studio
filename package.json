{
  "name": "root",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "tsc": "tsc --noEmit",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"./**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "lint": "eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "check:node": "check-node-version --node 18 --pnpm 8",
    "preinstall": "npm run check:node"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/dashboard": "^3.1.5",
    "@sanity/vision": "^3.15.1",
    "check-node-version": "^4.2.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-is": "^18.2.0",
    "sanity": "^3.15.1",
    "sanity-plugin-iframe-pane": "^2.5.4",
    "sanity-plugin-markdown": "^4.1.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^3.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "easymde": "^2.18.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "vercel": "^31.2.3"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
}
